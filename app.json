{
    "expo": {
        "name": "my-multipage-app",
        "slug": "my-multipage-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/images/icon.png",
        "scheme": "mymultipageapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": false,
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyCSwbEoeGMoOugY47K5ScG64LR0nxlSdWw"
                }
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyCSwbEoeGMoOugY47K5ScG64LR0nxlSdWw"
                }
            },
            "edgeToEdgeEnabled": true
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./src/assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./src/assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
